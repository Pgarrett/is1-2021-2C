!methodRemoval: Portfolio #printTransferNet stamp: 'PhG 10/20/2021 20:25:43'!
printTransferNet
	
	|transferNet|
	
	transferNet := 0.

	accounts do: [:account | transferNet := transferNet + account printTransferNet.	].

	^transferNet.!

!methodRemoval: ReceptiveAccount #printTransferNet stamp: 'PhG 10/20/2021 20:26:04'!
printTransferNet

	|transferNet|
	
	transferNet := 0.
	transactions do: [:transaction | 
		(transaction isKindOf: OriginWire) ifTrue: [transferNet := transferNet - transaction value].
		(transaction isKindOf: DestinationWire) ifTrue: [transferNet := transferNet + transaction value].
	].

	^transferNet.!
