!Fraccion methodsFor: 'arithmetic operations' stamp: 'PhG 9/9/2021 21:36:51' prior: 50873915 overrides: 50873723!
+ anAdder 
	
	| newNumerator newDenominator |
	
	
	(anAdder type = #Entero) ifTrue: [^self + (Fraccion with: anAdder over: 1)].
		
	newNumerator := (numerator * anAdder denominator) + (denominator * anAdder numerator).
	newDenominator := denominator * anAdder denominator.
	
	^newNumerator / newDenominator ! !

